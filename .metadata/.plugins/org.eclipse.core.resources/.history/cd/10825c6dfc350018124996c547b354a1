package livraria.dominio;

import java.util.Date;

public class Condicao extends EntidadeDominio {
	private String descricao, estado;
	private int idModificador;
	private Date dataModificacao;
	private CategoriaEstado categoria;
	
	public String getDescricao() {
		return descricao;
	}
	public void setDescricao(String descricao) {
		this.descricao = descricao;
	}
	public String getEstado() {
		return estado;
	}
	public void setEstado(String estado) {
		this.estado = estado;
	}
	public int getIdModificador() {
		return idModificador;
	}
	public void setIdModificador(int idModificador) {
		this.idModificador = idModificador;
	}
	public Date getDataModificacao() {
		return dataModificacao;
	}
	public void setDataModificacao(Date dataModificacao) {
		this.dataModificacao = dataModificacao;
	}
	public CategoriaEstado getCategoria() {
		return categoria;
	}
	public void setCategoria(CategoriaEstado categoria) {
		this.categoria = categoria;
	}
}
